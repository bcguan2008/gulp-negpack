{{if (debug) == 'true'}}
<button class="btn-reload fui-btn" onclick="location.reload()">刷新</button>
{{/if}}
{{if ((visitorPicAuth == "open" || visitorPicAuth == "auth") && (visitorPriceAuth == "open" || visitorPriceAuth == "auth")) && isMemberSupply == "false"}}
<div class="m-detail" data-wing="layout" data-name="page">
    <div class="region-custom region-detail-mobile-property region-blank" data-region-config='{"name":"region-detail-mobile-property","direction":"vertical"}'>
        <span class="placeholder fd-hide">region-detail-mobile-property</span>
        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('gallery')}}
            </div>
        </div>
        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('title')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('price')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('activity')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('activityPromotion')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('buttonZone')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('logistics')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('sku')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('preferential')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('supplierSupport')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('tradeWay')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('guarantee')}}
            </div>
        </div>

        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('actionOperator')}}
            </div>
        </div>
    </div>

    <div id="d-swipe-feature" class="d-swipe-feature m-detail-tabs">
        <ul class="swipe-nav">
            <li>产品属性</li>
            <li>评价({{rateTotals}})</li>
            <li>同店推荐</li>
        </ul>
        <div class="region-custom region-detail-mobile-feature region-blank swipe-content" data-region-config='{"name":"region-detail-mobile-feature","direction":"vertical"}'>
            <!-- <span class="placeholder fd-hide">region-detail-mobile-feature</span> -->
            <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
                <div class="widget-custom-container">
                    {{=wing.view('attributes')}}
                </div>
            </div>
            <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
                <div class="widget-custom-container">
                    <!-- offerRemark -->
                </div>
            </div>
            <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
                <div class="widget-custom-container">
                    <!-- commendOffer -->
                </div>
            </div>
        </div>
    </div>
</div>
{{else}}
<div class="m-detail m-detail-noauth" data-wing="layout" data-name="page">
    <div class="region-custom region-detail-mobile-private region-blank swipe-content" data-region-config='{"name":"region-detail-mobile-private","direction":"vertical"}'>
        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('private')}}
            </div>
        </div>
        <div class="widget-custom m-detail-demo" data-widget-name="m-detail-demo">
            <div class="widget-custom-container">
                {{=wing.view('actionOperator')}}
            </div>
        </div>
    </div>
</div>
{{/if}}