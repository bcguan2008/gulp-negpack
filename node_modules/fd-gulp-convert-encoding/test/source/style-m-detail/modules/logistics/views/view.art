<div class="m-detail-logistics">
    <div class="d-header">
    	<span class="d-title">物流说明</span>
    </div>
    <div class="d-content">

        {{if (isFreePostage == "true")}}

        <ul>
            <li>限时包邮</li>
            <li class="d-logistics-timer" id="d-logistics-timer">
                <i class="icon-clock"></i>剩余 <em class="day"></em>天 <em class="hour"></em>:<em class="minute"></em>:<em class="second"></em>
            </li>
            <li>{{freightInfo.location}} 发货</li>
        </ul>

        {{else if (freeDeliverFee == "true")}}
        <ul>
            <li>卖家承担运费</li>
            <li>{{freightInfo.location}} 发货</li>
        </ul>
        {{else if (freightInfo.deliveryFee == "STATED" && carriageDetail)}}
        <ul>
            <li>查看运费说明</li>
        </ul>
        <a class="d-showup" href="#">展开</a>

        {{else if (freightInfo.deliveryFee == "TEMPLATED")}}
        <ul>
            {{each freightCost as cost}}
            {{if (!(cost.subTemplateType=="2" && cashOnDeliveryOffer!="true"))}}
            <li>{{cost.subTemplateName}}：<span class="fd-cny">&yen; {{cost.totalCost}}</span></li>
            {{/if}}
            {{/each}}
            <li>{{freightInfo.location}} {{if (showDestinationArea)}}送到 {{destinationArea}}{{/if}}</li>
        </ul>
        <a class="d-showup" href="#">展开</a>

        {{else if (cashOnDeliveryOffer)}}

        <ul>
            <li>货到付款</li>
            <li>{{freightInfo.location}}  发货</li>
        </ul>
        {{/if}}

    </div>
</div>
