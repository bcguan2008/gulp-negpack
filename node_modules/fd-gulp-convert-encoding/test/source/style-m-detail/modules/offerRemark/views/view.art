<div class="m-detail-offer-remark m-detail-tab">
    <div class="d-content">
       	<div class="satisfaction">
    		<div class="baseinfo">
     			总体满意度：<span class="num">{{rateAverageStarLevel}}</span> <i class="icon-star star{{starLevel}}"></i>   			
    		</div>
    		<div class="moreinfo">
    			<div class="hd">总体满意度：</div>
    			<div class="bd">
    				<div class="remark-detail">
    					<div class="star-level">
    						<span class="level">{{rateAverageStarLevel}}</span>
    						<i class="icon-star star{{starLevel}}"></i>
    					</div>
    					<ul class="description">
                            {{each rateDsrItems as rateDsr}}
    						<li>{{rateDsr.shortName}} {{rateDsr.avgScore}}<i class="{{getDirection(rateDsr.indScorePer)}}"></i><span>{{if (rateDsr.indScorePer != 0)}}{{rateDsr.indScorePer}}%{{else}}持平{{/if}} </span></li>
                            {{/each}}
    					</ul>
    				</div>
                    {{if (rateImprStats && rateImprStats.length>0)}}
    				<div class="buyer-impression">
    					{{each rateImprStats as imprStats}}
                        <dl>
    						<dt>{{imprStats.name}}：</dt>
    						<dd>
    							<ul>
                                    {{each imprStats.impression as impression}}
    								<li class="level-{{$index+1}}" data-percent="{{impression.percent}}"><span class="arrow"></span><span class="text">{{impression.name}} {{impression.percent}}%</span></li>
                                    {{/each}}
    							</ul>
    						</dd>
    					</dl>
                        {{/each}}
    				</div>
                    {{/if}}
    			</div>
    		</div>
    	</div>
    	<div class="level-filter">
    		<ul>
    			<li class="curr" data-level="0">全部</li>
    			<li data-level="7">四五星({{rateStarLevel45}})</li>
    			<li data-level="3">三星({{rateStarLevel3}})</li>
    			<li data-level="6">一二星({{rateStarLevel12}})</li>
    		</ul>
    	</div>

        <!-- remark list -->
        <div id="remark-list-container">
                    
        </div>
    </div>
</div>