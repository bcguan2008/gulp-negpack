<div class="m-detail-logistics-info">

	<div class="d-head">
		<h3>物流说明</h3>
	</div>

	<div class="d-content">
		<div class="d-logisticsinfo-content">
			{{if (freightInfo.deliveryFee == "STATED" && carriageDetail)}}
				{{=wing.control('instruction')}}
			{{else if (freightInfo.deliveryFee == "TEMPLATED")}}
				{{=wing.control('templated')}}
			{{/if}}

		</div>
	</div>
	{{if (freightInfo.deliveryFee == "TEMPLATED")}}
	<div class="d-bottom">
		<div class="priceinfo">
		</div>
<!-- 		<div class="button">
			<button class="fui-btn-important">确 定</button>
		</div>
 -->	</div>         
	{{/if}}

</div>