<div class="m-detail-attributes m-detail-tab" data-mod-config='{"name":"产品属性"}'>
    <div class="d-content">
        <ul class="attributes">
            {{each productFeatureList as feature}}
            <li><span class="name">{{feature.name}}</span><span class="property">{{feature.value}}{{if (feature.unit)}}({{feature.unit}}){{/if}}</span></li>
            {{/each}}
        </ul>
    </div>
</div>