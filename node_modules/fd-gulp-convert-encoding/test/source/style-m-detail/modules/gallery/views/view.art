<div class="m-detail-gallery">
    <div class="d-content">
        {{if (imageList && imageList.length>0)}}
        <div id="d-swipe" class="d-swipe">
            <div class="swipe-content">
                {{each imageList as image}}
                <div class="swipe-pane"><img src="http://img.china.alibaba.com/cms/upload/other/lazyload.png" swipe-lazy-src="{{image.size310x310URL}}" class="swipe-image"></div>
                {{/each}}
            </div>
            <ul class="swipe-nav">
                {{each imageList as image}}
                <li></li>
                {{/each}}
            </ul>
        </div>
        <div id="d-swipe-popup" class="d-swipe-popup fd-hide">
            <div class="swipe-content">
                {{each imageList as image}}
                <div class="swipe-pane"><img src="http://img.china.alibaba.com/cms/upload/other/lazyload.png" swipe-lazy-src="{{image.originalImageURI}}" class="swipe-image"></div>
                {{/each}}
            </div>
            <ul class="swipe-nav fd-hide">
                {{each imageList as image}}
                <li></li>
                {{/each}}
            </ul>
            <p class="page-num"><span class="cur-page">1</span>/{{imageList.length}}</p>
        </div>
        {{else}}
        <div class="nopic">
            <img src="{{wing.path}}/modules/gallery/imgs/nopic.png" alt="暂无图片" />
        </div>
        {{/if}}

        {{if (isLimitedGroup == "true")}}
        <i class="icon-limited-group">伙拼</i>
        {{/if}}

        {{if (isCentralized == "true")}}
        <i class="icon-centralized">企业集采</i>
        {{/if}}

        {{=wing.control('fav')}}
        {{=wing.control('share')}}
    </div>
</div>