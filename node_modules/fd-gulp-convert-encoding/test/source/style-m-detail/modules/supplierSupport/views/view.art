{{if (isOfferSupportOnlineTrade == "true" && isSupportMix == "true")}}
<div class="m-detail-supplier-support">
    <div class="d-header"><span class="d-title">批发方式</span></div>
    <div class="d-content">
        <p class="hunpi">
            <i class="icon-hunpi">混批</i>
            {{if (mixAmount && mixAmount * 1 > 0 && mixNumber && mixNumber * 1 > 0)}}
            <span>全店满<em class="value">{{mixAmount}}</em>元或<em class="value">{{mixNumber}}</em>{{unit}}可混批采购{{if (mixBegin && mixBegin * 1 > 0)}}，本商品<em class="value">{{mixBegin}}</em>{{unit}}起批{{/if}}</span>
            {{else if (mixAmount && mixAmount * 1 > 0)}}
            <span>全店满<em class="value">{{mixAmount}}</em>元可混批采购{{if (mixBegin && mixBegin * 1 > 0)}}，本商品<em class="value">{{mixBegin}}</em>{{unit}}起批{{/if}}</span>
            {{else if (mixNumber && mixNumber * 1 > 0)}}
            <span>全店满<em class="value">{{mixNumber}}</em>{{unit}}可混批采购{{if (mixBegin && mixBegin * 1 > 0)}}，本商品<em class="value">{{mixBegin}}</em>{{unit}}起批{{/if}}</span>
            {{/if}}
        </p>
    </div>
</div>
{{/if}}