<div class="m-detail-commend-offer m-detail-tab">
    <div class="d-header"><span class="d-title">供应商其他相关产品</span></div>
    <div class="d-content">
        <div class="d-irecom">
            {{if (irecomData && irecomData.length)}}
            <ul class="list-product">
                {{each irecomData as item}}
                <li {{if ($index + 1) % perItem == 0}}class="last-col"{{/if}} data-ctr='{"objectId": "{{item.offerId}}","alg":"{{item.alg}}"}'>
                    <dl class="cell-product">
                        <dt class="vertical-img">
                            <a target="_blank" title="{{item.subject}}" class="box-img" href="{{getMobileDetailUrl(item.offerId)}}" data-trace="{{dmtrackPic}}" data-clickstat='{"objectType":"{{objectType}}","pid":"{{pid}}","objectId":"{{item.offerId}}","alg":"{{item.alg}}"}'>
                                <img alt="{{item.subject}}" src="{{getOfferImageUrl(item.offerImageUrl, 100)}}" />
                            </a>
                        </dt>
                        <dd class="description"><a target="_blank" title="{{item.subject}}" href="{{getMobileDetailUrl(item.offerId)}}" data-trace="{{dmtrackTitle}}" data-clickstat='{"objectType":"{{objectType}}","pid":"{{pid}}","objectId":"{{item.offerId}}","alg":"{{item.alg}}"}'>{{fitLength(item.subject, 34)}}</a></dd>
                        <dd class="price">
                        {{if (item.rmbPrice)}}
                        <span class="fd-cny">&yen;</span><em class="value">{{item.rmbPrice}}</em>
                        {{else if (item.foreignPrice)}}
                        <em class="value">{{item.foreignPrice}}{{item.foreignCurrency}}</em>
                        {{else}}
                        <em class="value no-pirce">价格面议</em>
                        {{/if}}
                        </dd>
                    </dl>
                </li>
                {{/each}}
            </ul>
            {{else}}
            暂无推荐
            {{/if}}
        </div>
    </div>
</div>
