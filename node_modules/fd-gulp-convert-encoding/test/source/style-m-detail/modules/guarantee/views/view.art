{{if (showGuarantee == "true")}}
<div class="m-detail-guarantee">
    <div class="d-header"><span class="d-title">买家保障</span></div>
    <div class="d-content">
        {{if (guaranteeLogoList && guaranteeLogoList.length > 0)}}
        <ul class="guarantee-type">
            {{each guaranteeLogoList as logoList}}
            <li class="" style="background:url('{{logoList.logoUrl}}') no-repeat;">
                {{logoList.serviceName}}
            </li>
            {{/each}}
        </ul>
        {{else}}
        <p class="guarantee-default-txt">该卖家支持先行赔付</p>
        {{/if}}
    </div>
</div>
{{/if}}